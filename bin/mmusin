#!/bin/env python

import socket
import sys

host = sys.argv[1]
port = int(sys.argv[2])

sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
sock.connect((host, port))

args = ""
for i in range(3, len(sys.argv)):
    args += sys.argv[i] + " "

sock.sendall(bytes(args, "utf-8"))

while 1:
    data = sock.recv(4096)
    if not data:
        break
    print(data.decode("utf-8"))

sock.close()
